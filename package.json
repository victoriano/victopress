{
  "name": "victopress",
  "version": "0.1.0",
  "description": "A files-first photo gallery CMS",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "prebuild": "bun run scripts/generate-sample-content.ts && bun run scripts/generate-content-manifest.ts",
    "build": "remix vite:build",
    "dev": "remix vite:dev",
    "start": "wrangler pages dev ./build/client",
    "typecheck": "tsc",
    "preview": "wrangler pages dev ./build/client",
    "deploy": "wrangler pages deploy ./build/client",
    "cf-typegen": "wrangler types",
    "generate-content": "bun run scripts/generate-sample-content.ts",
    "generate-manifest": "bun run scripts/generate-content-manifest.ts",
    "sync-to-r2": "bun run scripts/sync-local-to-r2.ts",
    "sync-from-r2": "bun run scripts/sync-r2-to-local.ts"
  },
  "dependencies": {
    "@dnd-kit/core": "^6.3.1",
    "@dnd-kit/sortable": "^10.0.0",
    "@dnd-kit/utilities": "^3.2.2",
    "@remix-run/cloudflare": "^2.15.2",
    "@remix-run/cloudflare-pages": "^2.15.2",
    "@remix-run/react": "^2.15.2",
    "exifr": "^7.1.3",
    "gray-matter": "^4.0.3",
    "isbot": "^5.1.17",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.962.0",
    "@cloudflare/workers-types": "^4.20241205.0",
    "@remix-run/dev": "^2.15.2",
    "@types/js-yaml": "^4.0.9",
    "@types/react": "^18.3.14",
    "@types/react-dom": "^18.3.2",
    "autoprefixer": "^10.4.20",
    "postcss": "^8.4.49",
    "tailwindcss": "^3.4.16",
    "typescript": "^5.7.2",
    "vite": "^6.0.5",
    "vite-tsconfig-paths": "^5.1.4",
    "wrangler": "^4.54.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
